<h1>Lists#index</h1>
<p>Find me in app/views/lists/index.html.erb</p>
<br>

<% if @lists.any? %>
    <% @lists.each do |list| %>
        <h1>list.name</h1>

        <% if list.items.any? %>
            <% list.items do |item| %>
                <h1>item.name</h1>

            <% end %>
        <% end %>

    <% end %>
<% end %>

